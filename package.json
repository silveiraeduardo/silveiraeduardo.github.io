{
  "name": "@ksakira10/silveiraeduardo.github.io",
  "version": "1.0.0",
  "private": true,
  "author": "Lucas Akira Uehara <akirauehara@hotmail.com>",
  "scripts": {
    "prepare": "pnpm husky install",
    "dev": "pnpm next dev",
    "build": "pnpm next build",
    "start": "pnpm next start",
    "sitemap": "pnpm next-sitemap",
    "export": "pnpm sitemap && pnpm next export",
    "lint": "pnpm next lint --ext .ts,.tsx --fix",
    "format": "pnpm prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "publish": "pnpm ci:publish",
    "serve": "pnpm build && pnpm export && pnpm http-server ./out/ -p3000 -gb -o /"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.9.5",
    "@vanilla-extract/next-plugin": "^2.1.1",
    "@vanilla-extract/sprinkles": "^1.5.1",
    "clsx": "^1.2.1",
    "next": "latest",
    "next-pwa": "^5.6.0",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@changesets/cli": "^2.26.0",
    "@jamesives/github-pages-deploy-action": "^4.4.1",
    "@svgr/webpack": "^6.5.1",
    "@types/node": "17.0.23",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "8.12.0",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "file-loader": "^6.2.0",
    "http-server": "^14.1.1",
    "husky": "^7.0.4",
    "next-sitemap": "^2.5.28",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "sharp": "^0.31.3",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0"
  }
}
